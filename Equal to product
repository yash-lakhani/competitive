#include <iostream>
using namespace std;

int main() {
	//code
	int T;
	cin>>T;
	int N,P;
	
	for(int i=0;i<T;i++){
	    cin>>N>>P;
	    int a[N];
	    for(int j=0;j<N;j++){
	        cin>>a[j];
	    }
	    for(int x=0;x<N;x++){
	        for(int y=0;y<N;y++){
	            if(a[x]*a[y]  == P){
	                cout<<"Yes"<<endl;
	                goto stop;
	            }
	            else if(x==N-1 && y == N-1){
	                cout<<"No"<<endl;
	            }
	        }
	        
	    }
	    stop:;
	    
	}
	return 0;
}
